  let content = to_markdown("# H1\n### H3 (skipped H2)\n")
  | let result = md001(content)
  | assert_eq(len(result), 1)
  | assert_eq(result[0]["rule"], "MD001")
  | assert_eq(result[0]["severity"], "error")
end

